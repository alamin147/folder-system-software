{
  "name": "visual-file-system-explorer",
  "version": "1.0.0",
  "description": "A beautiful canvas-based file system explorer built with MERN stack",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "file-system",
    "canvas",
    "react",
    "monaco-editor",
    "drag-drop",
    "mern-stack"
  ],
  "author": "Visual File System Explorer Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/visual-file-system-explorer.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/visual-file-system-explorer/issues"
  },
  "homepage": "https://github.com/your-username/visual-file-system-explorer#readme"
}
